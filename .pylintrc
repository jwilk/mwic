[MASTER]
load-plugins = pylint.extensions.check_elif

[MESSAGES CONTROL]
disable =
    bad-builtin,
    bad-continuation,
    fixme,
    inconsistent-return-statements,
    invalid-name,
    locally-disabled,
    no-self-use,
    redefined-variable-type,
    similarities,
    too-few-public-methods,
    too-many-branches,
    too-many-locals,
    too-many-statements,

[BASIC]
no-docstring-rgx = .*

[REPORTS]
reports = no
msg-template = {C}: {path}:{line}: {symbol} [{obj}] {msg}

[FORMAT]
max-line-length = 120
expected-line-ending-format = LF

